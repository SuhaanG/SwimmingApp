# Tkinter setup
root = tk.Tk()
root.title("Swim Time Standards")
root.geometry("500x400")
root.configure(bg="#ADD8E6")  # Aesthetic background color

# Dropdowns for age group and event and gender

gender_var = tk.StringVar()
gender_label = tk.Label(root, text="Select Gender:", bg="#ADD8E6")
gender_label.pack()
gender_dropdown = ttk.Combobox(root, textvariable=gender_var, values=["Male", "Female"])
gender_dropdown.pack()

age_var = tk.StringVar()
age_label = tk.Label(root, text="Select Age Group:", bg="#ADD8E6")
age_label.pack()
age_dropdown = ttk.Combobox(root, textvariable=age_var, values=["10 and under", "11-12", "13-14", "15-16", "17-18"])
age_dropdown.pack()

event_var = tk.StringVar()
event_label = tk.Label(root, text="Select Event:", bg="#ADD8E6")
event_label.pack()
event_dropdown = ttk.Combobox(root, textvariable=event_var, values=[
    "50-yard Freestyle", "100-yard Freestyle", "200-yard Freestyle", "500-yard Freestyle", "1000-yard Freestyle", "1650-yard Freestyle", "50-yard Backstroke",
    "100-yard Backstroke", "200-yard Backstroke", "50-yard Breaststroke", "100-yard Breaststroke", "200-yard Breaststroke",
    "50-yard Butterfly", "100-yard Butterfly", "200-yard Butterfly", "100-yard IM", "200-yard IM", "400-yard IM"
])
event_dropdown.pack()

# Time entry
time_label = tk.Label(root, text="Enter Your Time (seconds):", bg="#ADD8E6")
time_label.pack()
time_entry = tk.Entry(root)
time_entry.pack()

# Evaluate button
evaluate_button = tk.Button(root, text="Evaluate", command=evaluate_time)
evaluate_button.pack()

# Result label
result_label = tk.Label(root, text="", bg="#ADD8E6")
result_label.pack()

root.mainloop()


