import tkinter as tk
from tkinter import ttk, messagebox

def evaluate_time():
    age_group = age_var.get()
    event = event_var.get()
    gender = gender_var.get()
    time = float(time_entry.get())


#Male
   #10 & Under 

    if age_group == "10 and under" and event == "50-yard Freestyle" and gender == "Male": # Done
        if time <= 27.49:
            result_label.config(text="Your time puts you in the AAAA category.")
